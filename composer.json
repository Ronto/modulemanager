{
    "name": "levare/modules",
    "description": "A Module Manager for Laravel 4",
    "keywords": ["laravel", "module", "modulemanager"],
    "homepage": "https://github.com/LevareCMS/modulemanager",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Florian Uhlrich",
            "email": "f.uhlrich@levare-cms.de"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x"
    },
    "autoload": {
        "files": [
            "src/Levare/Modules/helpers.php"
        ],
        "psr-0": {
            "Levare\\Modules": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan config:publish levare/modules"
        ],
        "post-update-cmd": [
            "php artisan config:publish levare/modules"
        ]
    },
    "minimum-stability": "dev"
}